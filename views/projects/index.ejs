<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>Projects!</h1>
    <form action="/sessions?_method=DELETE" method="POST">
      <input type="submit" value="Logout" />
    </form>
    <a href="/projects/new">+ Project</a>
    <div>
      <% for (let project of projects) { %>
        <a href="/projects/<%= project.id %>">
          <div class="">
            <%= project.title %>
          </div>
        </a>
      <% } %>
    </div>
  </body>
</html>
